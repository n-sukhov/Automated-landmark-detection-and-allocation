<?xml version="1.0" ?>
<robot name="yolo_robot" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="base_link"/>
    
  <link name="platform">
    <inertial>
      <mass value="5.0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
    </inertial>

    <visual>
      <origin xyz="0 0 0.045" rpy="0 0 0" />
      <geometry>
        <box size="0.2 0.16 0.09" />
      </geometry>
      <material name="grey">
        <color rgba="0.625 0.625 0.625 1" />
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 1" rpy="0 0 0" />
      <geometry>
        <box size="0.2 0.16 0.097" />
      </geometry>
    </collision>
  </link>

  <link name="camera_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.05 0.05 0.05" />
      </geometry>
      <material name="grey">
        <color rgba="0.625 0.625 0.625 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.095" rpy="0 0 0" />
      <geometry>
        <box size="0.05 0.05 0.05" />
      </geometry>
    </collision>
  </link>

  <link name="left_wheel">
    <inertial>
      <mass value="3.0" />
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <cylinder length="0.03" radius="0.07" />
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <cylinder length="0.03" radius="0.07" />
      </geometry>
    </collision>
  </link>

  <link name="right_wheel">
    <inertial>
      <mass value="3.0" />
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <cylinder length="0.03" radius="0.07" />
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <cylinder length="0.03" radius="0.07" />
      </geometry>
    </collision>
  </link>

  <link name="caster_wheel">
    <inertial>
        <mass value="1.0" />
        <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005" />
    </inertial>
    <visual>
        <origin xyz="0 0 0" rpy="0 1.5708 0" />
        <geometry>
            <sphere radius="0.045" />
        </geometry>
        <material name="grey">
            <color rgba="0.625 0.625 0.625 1" />
        </material>
    </visual>
    <collision>
        <origin xyz="0 0 0" rpy="0 1.5708 0" />
        <geometry>
            <sphere radius="0.045" />
        </geometry>
    </collision>
  </link>


  <joint name="dummy_joint" type="fixed">
    <parent link="base_link" />
    <child link="platform" />
  </joint>

  <joint name="caster_wheel_joint" type="fixed">
    <parent link="platform" />
    <child link="caster_wheel" />
    <origin xyz="0.08 0 0" rpy="0 0 0" />
  </joint>

  <joint name="camera_joint" type="fixed">
    <parent link="platform" />
    <child link="camera_link" />
    <origin xyz="0.1 0 0.1" rpy="0 0 0" />
  </joint>

  <joint name="left_wheel_joint" type="continuous">
    <parent link="platform" />
    <child link="left_wheel" />
    <origin xyz="-0.05 -0.095 0.025" rpy="0 0 -1.57075" />
    <axis xyz="1 0 0" />
  </joint>

  <joint name="right_wheel_joint" type="continuous">
    <parent link="platform" />
    <child link="right_wheel" />
    <origin xyz="-0.05 0.095 0.025" rpy="0 0 1.57075" />
    <axis xyz="1 0 0" />
  </joint>

</robot>
