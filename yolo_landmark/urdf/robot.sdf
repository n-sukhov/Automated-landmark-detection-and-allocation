          <izz>0.1</izz>
        </inertia>
      </inertial>

      <collision name='base_link_fixed_joint_lump__platform_collision'>
        <pose>0 0 1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.16 0.097</size>
          </box>
        </geometry>
      </collision>

      <collision name='base_link_fixed_joint_lump__camera_link_collision_1'>
