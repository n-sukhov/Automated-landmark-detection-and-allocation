            <radius>0.004</radius>
          </cylinder>
        </geometry>
      </collision>

      <collision name='base_link_fixed_joint_lump__lower_sphere_collision_3'>
        <pose>0.097 0 -0.045 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.004</radius>
          </sphere>
        </geometry>
      </collision>

      <visual name='base_link_fixed_joint_lump__platform_visual'>
        <pose>0 0 0.045 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.16 0.097</size>
          </box>
        </geometry>
        <material>
          <diffuse>0.625 0.625 0.625 1</diffuse>
          <ambient>0.625 0.625 0.625 1</ambient>
        </material>
      </visual>

      <visual name='base_link_fixed_joint_lump__camera_link_visual_1'>
        <pose>0.1 0 0.095 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
        <material>
          <diffuse>0.625 0.625 0.625 1</diffuse>
          <ambient>0.625 0.625 0.625 1</ambient>
