        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.004</radius>
          </cylinder>
        </geometry>
        <material>
          <diffuse>0.625 0.625 0.625 1</diffuse>
          <ambient>0.625 0.625 0.625 1</ambient>
        </material>
      </visual>

