<sdf version='1.11'>
  <model name='yolo_robot'>
    <plugin name='gz::sim::systems::Sensors' filename='gz-sim-sensors-system'>
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin name='Image Display' filename='ImageDisplay'>
      <gz-gui>
        <property key='state' type='string'>docked</property>
      </gz-gui>
      <topic>/camera/image_raw</topic>
    </plugin>
    <plugin name="ros2_control" filename="libgz_ros2_control.so">
      <ros>
        <namespace>/</namespace>
        <parameter name="robot_description">robot_description</parameter>
      </ros>
    </plugin>

    <link name="base_link"/>    
    
    <link name="platform">
      <inertial>
        <pose>0 0 0.045 0 0 0</pose>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>

      <visual name="platform_visual">
        <pose>0 0 0.045 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.16 0.09</size>
          </box>
        </geometry>
        <material>
          <ambient>0.625 0.625 0.625 1</ambient>
          <diffuse>0.625 0.625 0.625 1</diffuse>
        </material>
      </visual>

      <collision name="platform_collision">
        <pose>0 0 1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.16 0.097</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name="camera_link">
      <pose relative_to='camera_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.002</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.002</iyy>
          <iyz>0</iyz>
          <izz>0.002</izz>
        </inertia>
      </inertial>
      <visual name="camera_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
        <material>
          <ambient>0.625 0.625 0.625 1</ambient>
          <diffuse>0.625 0.625 0.625 1</diffuse>
        </material>
      </visual>
      <collision name="camera_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
      </collision>
      <sensor name='camera' type='camera'>
        <pose>0.1 0 0.1 0 0 0</pose>
        <topic>/camera/image_raw</topic>
        <update_rate>30</update_rate>
        <enable_metrics>false</enable_metrics>
        <camera name='__default__'>
          <pose>0 0 0 0 0 0</pose>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>720</width>
            <height>512</height>
            <format>RGB_INT8</format>
            <anti_aliasing>4</anti_aliasing>
          </image>                                                                                                                                                                                                            
          <camera_info_topic>__default__</camera_info_topic>
          <trigger_topic></trigger_topic>
          <triggered>false</triggered>
          <clip>
            <near>0.1</near>
            <far>100</far>
          </clip>                                                                                                                                                                                                           
          <save enabled='false'>
            <path>__default__</path>                                                                                                                                                    
          </save>
          <visibility_mask>4294967295</visibility_mask>
          <noise>
            <type>none</type>
            <mean>0</mean>
            <stddev>0</stddev>
          </noise>
          <distortion>
            <k1>0</k1>
            <k2>0</k2>
            <k3>0</k3>
            <p1>0</p1>
            <p2>0</p2>
            <center>0.5 0.5</center>
          </distortion>
          <lens>
            <type>stereographic</type>
            <scale_to_hfov>true</scale_to_hfov>
            <cutoff_angle>1.5707963267948966</cutoff_angle>
            <env_texture_size>256</env_texture_size>
          </lens>
          <optical_frame_id></optical_frame_id>
        </camera>
      </sensor>
    </link>

    <link name="left_wheel">
      <pose relative_to='left_wheel_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 1.5708 0</pose>
        <mass>3.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="left_wheel_visual">
        <pose>0 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>
      <collision name="left_wheel_collision">
        <pose>0 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="right_wheel">
      <pose relative_to='right_wheel_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 1.5708 0</pose>
        <mass>3.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="right_wheel_visual">
        <pose>0 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>
      <collision name="right_wheel_collision">
        <pose>0 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="caster_wheel">
      <pose relative_to='caster_wheel_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.005</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.005</iyy>
          <iyz>0</iyz>
          <izz>0.005</izz>
        </inertia>
      </inertial>
      <visual name="caster_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.045</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>0.625 0.625 0.625 1</ambient>
          <diffuse>0.625 0.625 0.625 1</diffuse>
        </material>
      </visual>
      <collision name="caster_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.045</radius>
          </sphere>
        </geometry>
      </collision>
    </link>


    <joint name="dummy_joint" type="fixed">
      <parent>base_link</parent>
      <child>platform</child>
    </joint>

    <joint name="caster_wheel_joint" type="fixed">
      <pose relative_to='platform'>0.08 0 0 0 0 0</pose>
      <parent>platform</parent>
      <child>caster_wheel</child>
    </joint>

    <joint name="camera_joint" type="fixed">
      <pose relative_to='platform'>0.1 0 0.1 0 0 0</pose>
      <parent>platform</parent>
      <child>camera_link</child>
    </joint>

    <joint name="left_wheel_joint" type="revolute">
      <pose relative_to='platform'>-0.05 -0.095 0.025 0 0 -1.57075</pose>
      <parent>platform</parent>
      <child>left_wheel</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-inf</lower>
          <upper>inf</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <joint name="right_wheel_joint" type="revolute">
      <pose relative_to='platform'>-0.05 0.095 0.025 0 0 1.57075</pose>
      <parent>platform</parent>
      <child>right_wheel</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-inf</lower>
          <upper>inf</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

  </model>

</sdf>