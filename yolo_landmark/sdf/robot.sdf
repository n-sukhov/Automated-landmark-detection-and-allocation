<sdf version='1.11'>
  <model name='yolo_robot'>
    <plugin name='gz::sim::systems::Sensors' filename='gz-sim-sensors-system'>
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin name='Image Display' filename='ImageDisplay'>
      <gz-gui>
        <property key='state' type='string'>docked</property>
      </gz-gui>
      <topic>/camera/image_raw</topic>
    </plugin>

    <plugin name='gz_ros2_control::GazeboSimROS2ControlPlugin' filename='gz_ros2_control-system'>
      <ros>
        <namespace></namespace>
      </ros>
      <parameters>/home/ns/ros2_ws/src/yolo_landmark/config/diff_drive_controller.yaml</parameters>
    </plugin>

    <link name="base_link">
      <pose>0 0 0.2000000000000001 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>3</mass>
        <inertia>
          <ixx>0.05</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.17</iyy>
          <iyz>0.0</iyz>
          <izz>0.20</izz>
        </inertia>
      </inertial>

      <visual name="base_link_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.8 0.4 0.2</size>
          </box>
        </geometry>
        <material>
          <ambient>0.8 0.4 0.0 1.0</ambient>
          <diffuse>0.8 0.4 0.0 1.0</diffuse>
        </material>
      </visual>

      <collision name="base_link_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.8 0.4 0.2</size>
          </box>
        </geometry>
      </collision>
    </link>

    <joint name="lidar_joint" type="fixed">
      <pose relative_to='base_link'>0 0 0.15 0 0  0</pose>
      <parent>base_link</parent>
      <child>lidar_link</child>
    </joint>

    <link name="lidar_link">
      <pose relative_to='lidar_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.2</mass>
        <inertia>
          <ixx>0.02</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.02</iyy>
          <iyz>0.0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>

      <visual name="lidar_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.625 0.625 0.625 1</ambient>
          <diffuse>0.625 0.625 0.625 1</diffuse>
        </material>
      </visual>

      <collision name="lidar_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
      </collision>

      <sensor name='lidar' type='gpu_lidar'>
        <pose>0 0 0.2 0 0 0</pose>
        <topic>/scan</topic>
        <update_rate>30</update_rate>
        <enable_metrics>false</enable_metrics>
        <lidar>
          <frame_id></frame_id>
          <scan>
            <horizontal>
              <samples>1440</samples>
              <resolution>1</resolution>
              <min_angle>-3.141592653589</min_angle>
              <max_angle>3.141592653589</max_angle>
            </horizontal>
            <vertical>
              <samples>1</samples>
              <min_angle>0</min_angle>
              <max_angle>0</max_angle>
              <resolution>1</resolution>
            </vertical>
          </scan>
          <range>
            <min>1.0000000000000001e-05</min>
            <max>20</max>
            <resolution>0.015</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.01</stddev>
          </noise>
          <visibility_mask>4294967295</visibility_mask>
        </lidar>
      </sensor>
    </link>

    <joint name="camera_joint" type="fixed">
      <pose relative_to='base_link'>0.41 0 0 0 0 0</pose>
      <parent>base_link</parent>
      <child>camera_link</child>
    </joint>

    <link name="camera_link">
      <pose relative_to='camera_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.02</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.02</iyy>
          <iyz>0.0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>

      <visual name="camera_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.2 0.2</size>
          </box>
        </geometry>
        <material>
          <ambient>0.625 0.625 0.625 1</ambient>
          <diffuse>0.625 0.625 0.625 1</diffuse>
        </material>
      </visual>

      <collision name="camera_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.2 0.2</size>
          </box>
        </geometry>
      </collision>

      <sensor name='camera' type='camera'>
        <pose>0.41 0 0 0 0 0</pose>
        <topic>/camera/image_raw</topic>
        <update_rate>30</update_rate>
        <enable_metrics>false</enable_metrics>
        <camera name='__default__'>
          <pose>0 0 0 0 0 0</pose>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>720</width>
            <height>512</height>
            <format>RGB_INT8</format>
            <anti_aliasing>4</anti_aliasing>
          </image>                                                                                                                                                                                                            
          <camera_info_topic>/camera_info</camera_info_topic>
          <trigger_topic></trigger_topic>
          <triggered>false</triggered>
          <clip>
            <near>0.1</near>
            <far>100</far>
          </clip>                                                                                                                                                                                                           
          <save enabled='false'>
            <path>__default__</path>                                                                                                                                                    
          </save>
          <visibility_mask>4294967295</visibility_mask>
          <noise>
            <type>none</type>
            <mean>0</mean>
            <stddev>0</stddev>
          </noise>
          <distortion>
            <k1>0</k1>
            <k2>0</k2>
            <k3>0</k3>
            <p1>0</p1>
            <p2>0</p2>
            <center>0.5 0.5</center>
          </distortion>
          <lens>
            <type>stereographic</type>
            <scale_to_hfov>true</scale_to_hfov>
            <cutoff_angle>1.5707963267948966</cutoff_angle>
            <env_texture_size>256</env_texture_size>
          </lens>
          <optical_frame_id></optical_frame_id>
        </camera>
      </sensor>
    </link>

    <joint name="left_wheel_joint" type="revolute">
      <pose relative_to='base_link'>0.2 0.25 -0.1 -1.5707 0 0</pose>
      <parent>base_link</parent>
      <child>left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-inf</lower>
          <upper>inf</upper>
        </limit>
        <dynamics>
          <damping>0.05</damping>
        </dynamics>
      </axis>
    </joint>

    <link name="left_wheel">
      <pose relative_to='left_wheel_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.3</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.015</izz>
        </inertia>
      </inertial>

      <visual name="left_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>

      <collision name="left_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="right_wheel_joint" type="revolute">
      <pose relative_to='base_link'>0.2 -0.25 -0.1 -1.5707 0 0</pose>
      <parent>base_link</parent>
      <child>right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-inf</lower>
          <upper>inf</upper>
        </limit>
        <dynamics>
          <damping>0.05</damping>
        </dynamics>
      </axis>
    </joint>

    <link name="right_wheel">
      <pose relative_to='right_wheel_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.3</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.015</izz>
        </inertia>
      </inertial>

      <visual name="right_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>

      <collision name="right_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="caster_joint" type="fixed">
      <pose relative_to='base_link'>-0.3 0 -0.1 0 0 0</pose>
      <parent>base_link</parent>
      <child>caster</child>
    </joint>

    <link name="caster">
      <pose relative_to='caster_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>

      <visual name="caster_visual">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>1 1 1 1</ambient>
          <diffuse>1 1 1 1</diffuse>
        </material>
      </visual>

      <collision name="caster_collision">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

  </model>

</sdf>